{
    "require": {
        "php": "^7.4",
        "monei/monei-php-sdk": "^2.8",
        "php-di/php-di": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Monei\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "wp-coding-standards/wpcs": "^3.1"
    },
    "config": {
        "platform": {
            "php": "7.4.33"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
